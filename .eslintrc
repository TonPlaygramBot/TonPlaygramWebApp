{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": ["webapp/dist/**"],
  "plugins": ["react"],
  "rules": {}
}
